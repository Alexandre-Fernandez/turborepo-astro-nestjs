		"@nestjs/mongoose": "^9.2.0",
		"@nestjs/platform-express": "^9.0.0",
		"@nestjs/platform-fastify": "^9.1.2",
		"@nestjs/swagger": "^6.1.2",
		"@typegoose/typegoose": "^9.12.0",
		"mongoose": "^6.6.1",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "^7.5.5",
		"webpack": "^5.74.0"
	},
